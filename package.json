{
  "name": "careplace-backend",
  "version": "1.0.0",
  "description": "Careplace REST API Backend for Web and Mobile Applications.",
  "main": "./src/server.ts",
  "private": true,
  "nodemonConfig": {
    "ignore": [
      "./src/assets/data/*.json"
    ]
  },
  "scripts": {
    "start": "ts-node --project tsconfig.json -r tsconfig-paths/register build/server.js",
    "build": "rimraf ./build && tsc --project tsconfig.json",
    "dev": "nodemon --exec \"node --trace-warnings --experimental-modules -r ts-node/register -r tsconfig-paths/register\" ./src/server.ts",
    "docker-dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker-prod": "docker-compose -f docker-compose.prod.yml up --build",
    "non-docker-dev": "npm run dev",
    "stripe-cli-setup": "node ./scripts/stripe-cli-setup.js",
    "dev:stripe": "stripe listen --forward-to localhost:8080/v1/webhooks/stripe",
    "dev:ngrok": "npx ngrok http 8080 --host-header=rewrite --config=.ngrok/ngrok.yml",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "jenkins:test": "jenkins tests/test-dev.Jenkinsfile",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "lint:commit": "lint-staged",
    "commitmsg": "commitlint -E HUSKY_GIT_PARAMS",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "prettier-check": "prettier --check 'src/**/*.{js,jsx,ts,tsx}'",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:commit"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "yarn prettier",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CarelyPT/crm-backend.git"
  },
  "keywords": [
    "Careplace"
  ],
  "author": "CARELY, LDA.",
  "bugs": {
    "url": "https://github.com/CarelyPT/crm-backend/issues"
  },
  "homepage": "https://github.com/CarelyPT/crm-backend#readme",
